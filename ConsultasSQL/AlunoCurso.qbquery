CREATE TABLE ALUNO
(CODIGO INT AUTO_INCREMENT,
 MATRICULA VARCHAR(15),
 NOME VARCHAR(150),
 CODIGOCURSO INT,
 PRIMARY KEY (CODIGO),
 FOREIGN KEY (CODIGOCURSO) REFERENCES CURSO(CODIGO) )
ENGINE=INNODB;


